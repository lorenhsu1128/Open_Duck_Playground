<mujoco model="combined_terrain_backlash_scene">
  <include file="open_duck_mini_v2_backlash.xml"/>

  <compiler angle="radian" meshdir="assets" texturedir="assets"/>
  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
    <map znear="0.01"/>
    <quality shadowsize="4096"/>
  </visual>

  <asset>
    <texture name="grid" type="2d" builtin="checker" rgb1="0.1 0.2 0.3" rgb2="0.2 0.3 0.4" width="300" height="300" mark="edge" markrgb="0.2 0.3 0.4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance="0.2"/>

    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="1 1 1" rgb2="1 1 1" markrgb="0 0 0" width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance=".8"/>
    <hfield name="hfield" file="assets/hfield.png" size="10 10 .01 0.1"/>
  </asset>

  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1"/>

    <geom name="flat_floor" pos="0 0 0" size="10 10 0.1" type="plane" material="grid" condim="3"/>

    <!-- <geom name="rough_floor" pos="20 0 0" type="hfield" hfield="rough_terrain" material="grid" condim="3"/> -->
    <geom name="rough_floor" pos="20 0 0"  type="hfield" hfield="hfield" material="groundplane" contype="1" conaffinity="0" priority="1"
        friction="1.0"/>

